      "contributors": {
        "Ellemes": "Owner"
      },
      "contact": {
        "homepage": "https://modrinth.com/mod/expanded-storage",
        "issues": "https://github.com/Ellemes/expanded-storage",
        "sources": "https://github.com/Ellemes/ExpandedStorage"
      },
      "license": [
      ],
      "icon": "icon.png"
    }
  },
  "mixin": [
    "expandedstorage-common.mixin.json",
    "expandedstorage-quilt.mixin.json"
  ]
}
