{
  "schema_version": 1,
  "quilt_loader": {
    "group": "ellemes",
    "id": "expandedstorage",
    "version": "${version}",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "ellemes.expandedstorage.quilt.Main",
      "client_init": "ellemes.expandedstorage.quilt.Client",
      "fabric-datagen": "ellemes.expandedstorage.quilt.datagen.Main"
    },
    "depends": [
      {
        "id": "org.quiltmc:quilt_loader",
